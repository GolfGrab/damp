{
  "name": "consumer",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:dev": "nest start --watch ",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "check-types": "tsc --noemit",
    "eslint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint": "npm run eslint && npm run check-types",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma:db:pull": "npx prisma db pull",
    "prisma:generate": "npx prisma generate",
    "prisma:studio": "npx prisma studio",
    "nest:g:resource": "nest g resource && npm run lint",
    "start:email:dev": "cross-env CHANNEL_TYPE=EMAIL nest start --watch",
    "start:email:prod": "cross-env CHANNEL_TYPE=EMAIL node dist/main",
    "start:sms:dev": "cross-env CHANNEL_TYPE=SMS nest start --watch",
    "start:sms:prod": "cross-env CHANNEL_TYPE=SMS node dist/main",
    "start:slack:dev": "cross-env CHANNEL_TYPE=SLACK nest start --watch",
    "start:slack:prod": "cross-env CHANNEL_TYPE=SLACK node dist/main",
    "start:all:dev": "concurrently -n email,sms,slack -c yellow,green,blue,magenta \"npm run start:email:dev\" \"npm run start:sms:dev\" \"npm run start:slack:dev\"",
    "start:all:prod": "concurrently -n email,sms,slack -c yellow,green,blue,magenta \"npm run start:email:prod\" \"npm run start:sms:prod\" \"npm run start:slack:prod\""
  },
  "dependencies": {
    "@fastify/static": "^6.12.0",
    "@nestjs/common": "10.3.0",
    "@nestjs/config": "3.1.1",
    "@nestjs/core": "10.3.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/microservices": "^10.4.8",
    "@nestjs/platform-fastify": "10.3.0",
    "@nestjs/schedule": "^4.0.2",
    "@nestjs/swagger": "7.1.17",
    "@nodeteam/nestjs-prisma-pagination": "^1.0.6",
    "@prisma/client": "^5.22.0",
    "@slack/web-api": "^7.8.0",
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.10.4",
    "axios": "^1.6.8",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "csv-parse": "^5.5.5",
    "fastify": "^4.26.2",
    "js-base64": "^3.7.7",
    "lodash": "^4.17.21",
    "mssql": "10.0.1",
    "nest-typed-config": "^2.9.3",
    "nest-winston": "^1.9.4",
    "nestjs-prisma": "^0.23.0",
    "nestjs-spelunker": "^1.3.0",
    "nodemailer": "^6.9.16",
    "openid-client": "^5.6.5",
    "pako": "^2.1.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1",
    "twilio": "^5.4.0",
    "winston": "^3.13.0",
    "winston-daily-rotate-file": "^4.7.1",
    "yaml": "^2.4.1"
  },
  "devDependencies": {
    "@nestjs/cli": "10.3.0",
    "@nestjs/schematics": "10.1.0",
    "@nestjs/testing": "10.3.0",
    "@types/jest": "29.5.11",
    "@types/lodash": "^4.17.0",
    "@types/node": "18.11.9",
    "@types/nodemailer": "^6.4.17",
    "@types/pako": "^2.0.3",
    "@types/supertest": "6.0.2",
    "@typescript-eslint/eslint-plugin": "6.18.1",
    "@typescript-eslint/parser": "6.18.1",
    "concurrently": "^9.1.0",
    "cross-env": "^7.0.3",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "jest": "29.7.0",
    "prettier": "3.1.1",
    "prisma": "^5.22.0",
    "prisma-dbml-generator": "^0.12.0",
    "source-map-support": "0.5.21",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.3.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
